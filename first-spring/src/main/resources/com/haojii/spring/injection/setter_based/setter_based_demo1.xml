<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="foo1" name="foo1" class="com.haojii.spring.injection.setter_based.Foo">
		<property name="bar" ref="bar1"></property>
		<property name="baz" ref="baz1"></property>
	</bean>
	
	<bean id="bar1" class="com.haojii.spring.injection.setter_based.Bar"></bean>
	<bean id="baz1" class="com.haojii.spring.injection.setter_based.Baz"></bean>

</beans>
